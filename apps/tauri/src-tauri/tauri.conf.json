{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-starter",
  "version": "0.0.1",
  "identifier": "com.tauri-starter.app",
  "build": {
    "beforeDevCommand": "pnpm --filter web dev",
    "devUrl": "http://localhost:4132",
    "beforeBuildCommand": "pnpm --filter web build",
    "frontendDist": "../../web/out"
  },
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "title": "Tauri Starter",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
